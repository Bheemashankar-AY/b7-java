pipeline{
    agent any
    stages{
        stage('Clean'){
            steps{
                    sh 'mvn clean'
                }
            }
        stage('Install'){
            steps{
                    sh 'mvn install'
                }
            }
            post{
                always{
                    archiveArtifacts artifacts: 'target/**.jar', followSymlinks: false
                    junit 'target/surefire-reports/*.xml'					
                }
            }
        }
		stage('clean ws'){
		  setps{
		    cleanWs()
		}
	}
}
